extends _bootstrap
append styles
        // Custom styles for this template
        link(href='../stylesheets/styles.css', rel='stylesheet')
        link(href='../stylesheets/bootstrap.min.css', rel='stylesheet')
        link(href='../stylesheets/Info.css', rel ='stylesheet')
block body 
 .navbar-wrapper
  .container
   nav.navbar.navbar-inverse.navbar-static-top
    .container
     .navbar-header
       button(type="button" ,class="navbar-toggle collapsed", data-toggle="collapse", data-target="#navbar" ,aria-expanded="false", aria-controls="navbar")
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar						
       a(class="navbar-brand", href="#") IU-Know
     #navbar.navbar-collapse.collapse
       ul.nav.navbar-nav
        li
         a(href="http://localhost:3000/home") Home						
        li.active
         a(href="http://localhost:3000/gallery") Gallery
        li
         a(href="http://localhost:3000/forum") Forum
        li
         a(href="http://localhost:3000/contact") Contact Us
    // Wrap all page content here
    #wrap
      header.masthead
        //
          Carousel
          ==================================================
        #myCarousel.carousel.slide
          // Indicators
          ol.carousel-indicators
            li.active(data-target='#myCarousel', data-slide-to='0')
            li(data-target='#myCarousel', data-slide-to='1')
          .carousel-inner
            .item
              img(src='../images/File_2.jpeg')
            .item.active
              img(src='../images/File_7.jpeg')
          // /.carousel-inner
          // Controls
          a.left.carousel-control(href='#myCarousel', data-slide='prev')
            span.glyphicon.glyphicon-chevron-left
          a.right.carousel-control(href='#myCarousel', data-slide='next')
            span.glyphicon.glyphicon-chevron-right
        // /.carousel
          // /.nav-collapse
        // /.container
      // /.navbar
    #footer
      .container
        p.text-muted Picture Copyrights Stephy Matthew
    // script references
    script(src='//ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js')
    script(src='js/bootstrap.min.js')
    script(src='http://maps.googleapis.com/maps/api/js?sensor=false&extension=.js&output=embed')
    script(src='js/scripts.js')

